{
    "name": "icecat/icecat-integration",
    "description": "This plugin provides integrtaion of pimcore with Icecat",
    "type": "pimcore-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Faisal Afroz",
        "email": "faisalroz@mindstask.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "pimcore/pimcore": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IceCatBundle\\": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "IceCatBundle\\IceCatBundle"
            ]
        }
    }
}